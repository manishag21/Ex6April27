   "source": [
    "import pandas as pd\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.DataFrame(np.random.randint(1,100, 40).reshape(10, -1), columns=list('pqrs'), index=list('abcdefghij'))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "#init outputs\n",
    "nearest_rows=[]\n",
    "nearest_distance=[]\n",
    "#iterate rows.\n",
    "for i, row in df.iterrows():\n",
    "    curr=row\n",
    "    rest=df#Changed from df.drop(i)\n",
    "    e_dists ={}#init dists to store edclidean dists for current row.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "#iterate rest of rows for current row\n",
    "for j, contestant in rest.iterrows():\n",
    "    # compute euclidean dist and update e_dists\n",
    "    e_dists.update({j:round(np.linalg.norm(curr.values - contestant.values))})\n",
    "    #update nearest row to current row and the distance value\n",
    "    nearest_rows.append(max(e_dists, key=e_dists.get))\n",
    "    nearest_distance.append(max(e_dists.values()))\n",
    "    \n",
    "    "
   ]
